{
  "functions": {
    "source": "backend",
    "predeploy": [
      "npm --prefix \"%RESOURCE_DIR%\" run lint"
    ]
  },
  "hosting": {
    "public": "frontend/build",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "storage": {
    "rules": "storage.rules"
  }
}
